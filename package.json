{
  "name": "todomvc-spec",
  "version": "0.0.1",
  "description": "example cucumber spec for todomvc app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint features leadfoot page-objects plugins",
    "test": "eslint features leadfoot page-objects plugins && ./cucumber $CUCUMBER_ARGS",
    "selenium": "selenium-standalone install && selenium-standalone start",
    "check": "npm-check || true && nsp check",
    "checku": "npm-check -u || true",
    "preversion": "npm test && npm run check",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benjamine/todomvc-spec.git"
  },
  "keywords": [
    "spec",
    "bdd",
    "cucumber",
    "test",
    "acceptance"
  ],
  "author": "Benjamin Eidelman <beneidel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjamine/todomvc-spec/issues"
  },
  "homepage": "https://github.com/benjamine/todomvc-spec#readme",
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.6.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-fp": "^2.1.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "npm-check": "latest",
    "nsp": "latest"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.14.0",
    "bulk-require": "^1.0.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cucumber": "latest",
    "fetch-ponyfill": "^3.0.2",
    "fetch-wrap": "^0.0.6",
    "googleapis": "^15.0.0",
    "html-to-text": "^3.0.0",
    "http-proxy": "^1.16.2",
    "inbox": "^1.1.59",
    "js-yaml": "^3.6.1",
    "leadfoot": "^1.7.0",
    "lodash": "^4.15.0",
    "mimeparser": "^0.3.8",
    "open": "^0.0.5",
    "page-object-pattern": "^0.0.5",
    "people-names": "^1.0.15",
    "promise-retry": "^1.1.1",
    "selenium-standalone": "^5.9.0",
    "tcp-port-used": "^0.1.2",
    "underscore.string": "^3.3.4",
    "url2": "^1.0.4"
  }
}
